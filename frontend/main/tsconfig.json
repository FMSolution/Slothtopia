{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2019",
       "dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ],
 
}
